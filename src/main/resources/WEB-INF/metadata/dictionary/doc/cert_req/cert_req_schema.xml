<?xml version="1.0" encoding="UTF-8"?>
<SCHEMA xmlns="http://www.custsystems.com/2009/plugins"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.custsystems.com/2009/plugins /WEB-INF/metadata/xsd/custerp.xsd">
	<STATUS NAME="new">
		<ACTION NAME="for_send" ROLES="ROLE_ADMIN;ROLE_SECURITY_ADMIN">
			<PROPERTIES>
				<PROPERTY NAME="CAPTION" LVALUE="ACTIONS/for_send" />
				<PROPERTIES NAME="CUSTERP_ACTION">
					<PROPERTY NAME="CLASS_REF" VALUE="SIMPLE_DC_ACTION" />
					<PROPERTY NAME="IMAGE" IMAGE_REF="DEFAULT/UNDELETE" />
				</PROPERTIES>
			</PROPERTIES>
			<OPERATION CLASS_REF="CHANGE_STATUS" >
				<PROPERTIES>
					<PROPERTY NAME="STATUS" VALUE="for_send" />
				</PROPERTIES>
			</OPERATION>
		</ACTION>
	</STATUS>

	<STATUS NAME="for_send">
		<PROPERTIES>
			<PROPERTY NAME="CAPTION" LVALUE="STATUSES/for_send" />
			<PROPERTY NAME="STYLE" RESOURCE="system/docstyle" CVALUE="for_send" />
		</PROPERTIES>
		<ACTION NAME="new" ROLES="ROLE_ADMIN;ROLE_SECURITY_ADMIN">
			<PROPERTIES>
				<PROPERTY NAME="CAPTION" LVALUE="ACTIONS/new" />
				<PROPERTIES NAME="CUSTERP_ACTION">
					<PROPERTY NAME="CLASS_REF" VALUE="SIMPLE_DC_ACTION" />
					<PROPERTY NAME="IMAGE" IMAGE_REF="DEFAULT/UNDELETE" />
				</PROPERTIES>
			</PROPERTIES>
			<OPERATION CLASS_REF="CHANGE_STATUS" >
				<PROPERTIES>
					<PROPERTY NAME="STATUS" VALUE="new" />
				</PROPERTIES>
			</OPERATION>
		</ACTION>
		<ACTION NAME="end" SHOW_ORDER="8" ROLES="ROLE_ADMIN;ROLE_SECURITY_ADMIN">
			<PROPERTIES>
				<PROPERTY NAME="CAPTION" LVALUE="ACTIONS/end" />
				<PROPERTIES NAME="CUSTERP_ACTION">
					<PROPERTY NAME="CLASS_REF" VALUE="SIMPLE_DC_ACTION" />
					<PROPERTY NAME="IMAGE" IMAGE_REF="DOC/END" />
				</PROPERTIES>
			</PROPERTIES>
			<OPERATION CLASS_REF="CHANGE_STATUS" >
				<PROPERTIES>
					<PROPERTY NAME="STATUS" VALUE="end" />
				</PROPERTIES>
			</OPERATION>
		</ACTION>
	</STATUS>
</SCHEMA>