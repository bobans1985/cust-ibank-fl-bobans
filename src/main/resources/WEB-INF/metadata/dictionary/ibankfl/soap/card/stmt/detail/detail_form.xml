<?xml version="1.0" encoding="UTF-8"?>
<FORM xmlns="http://www.custsystems.com/2009/plugins"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.custsystems.com/2009/plugins /WEB-INF/metadata/xsd/custerp.xsd">
	<WIDGETS>

		<WIDGET NAME="LIST" CLASS_REF="LIST.PANEL">
			<COLUMNS>
				<COLUMN FIELDNAME="TRANS_AMOUNT" WIDTH="100px" ORDER="1"/>
				<COLUMN FIELDNAME="TRANS_CURR_CODE" WIDTH="60px" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANS_CURR_CODE_ISO" WIDTH="80px" ALIGN="center" ORDER="2"/>
				<COLUMN FIELDNAME="TRANS_DATE" WIDTH="100px" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANS_DATETIME" WIDTH="145px" ORDER="3"/>
				<COLUMN FIELDNAME="ACCOUNT_AMOUNT" WIDTH="120px" VISIBLE="false"/>
				<COLUMN FIELDNAME="ACCOUNT_CURR_CODE" WIDTH="60px" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="ACCOUNT_CURR_CODE_ISO" WIDTH="60px" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="POSTING_DATE" WIDTH="100px" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANSACTION" WIDTH="90px" VISIBLE="false"/>
				<COLUMN FIELDNAME="HOLD" WIDTH="90px" VISIBLE="false"/>
				<COLUMN FIELDNAME="DETAILS" WIDTH="200px" VISIBLE="false"/>
				<COLUMN FIELDNAME="POS_NAME" WIDTH="315" VISIBLE="true" ORDER="4"/>
				<COLUMN FIELDNAME="POS_MCC" WIDTH="90px" VISIBLE="false"/>
				<COLUMN FIELDNAME="POS_TERMINAL_ID" WIDTH="90px" VISIBLE="false"/>
			</COLUMNS>
		</WIDGET>

		<WIDGET NAME="LIST_COMPACT" CLASS_REF="GRID_COMPACT.PANEL">
			<COLUMNS>
				<COLUMN FIELDNAME="TRANS_AMOUNT" HFLEX="min"/>
				<COLUMN FIELDNAME="TRANS_CURR_CODE_ISO" HFLEX="min" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANS_CURR_CODE" HFLEX="min" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANS_DATE" HFLEX="min" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANS_DATETIME" HFLEX="min"/>
				<COLUMN FIELDNAME="ACCOUNT_AMOUNT" HFLEX="min" VISIBLE="false" />
				<COLUMN FIELDNAME="ACCOUNT_CURR_CODE_ISO" HFLEX="min" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="ACCOUNT_CURR_CODE" HFLEX="min" ALIGN="center" VISIBLE="false"/>
				<COLUMN FIELDNAME="POSTING_DATE" HFLEX="min" VISIBLE="false"/>
				<COLUMN FIELDNAME="TRANSACTION" HFLEX="min" VISIBLE="false"/>
				<COLUMN FIELDNAME="HOLD" HFLEX="min" VISIBLE="false"/>
				<COLUMN FIELDNAME="DETAILS" HFLEX="min" VISIBLE="false"/>
				<COLUMN FIELDNAME="POS_NAME" HFLEX="min" VISIBLE="true"/>
				<COLUMN FIELDNAME="POS_MCC" HFLEX="min" VISIBLE="false"/>
				<COLUMN FIELDNAME="POS_TERMINAL_ID" HFLEX="min" VISIBLE="false"/>
			</COLUMNS>
			<ACTIONS>
				<ACTIONGROUP NAME="DEFAULT.READONLY.JO_BAN_REFRESH"/>
			</ACTIONS>
		</WIDGET>

		<WIDGET NAME="VIEW" CLASS_REF="LINE">
			<COMPONENTS>
				<COMPONENT FIELDNAME="POSTING_DATE" CLASS_REF="DATE_LABEL"  VISIBLE="false"/>
				<COMPONENT FIELDNAME="ACCOUNT_AMOUNT" CLASS_REF="MONEY_LABEL"  VISIBLE="false"/>
				<COMPONENT FIELDNAME="POS_TERMINAL_ID" CLASS_REF="SIMPLE_LABEL"  VISIBLE="false"/>
				<COMPONENT FIELDNAME="POS_MCC" CLASS_REF="SIMPLE_LABEL"  VISIBLE="false"/>
				<COMPONENT FIELDNAME="POS_LATITUDE" CLASS_REF="SIMPLE_LABEL" VISIBLE="false"/>
				<COMPONENT FIELDNAME="POS_LONGITUDE" CLASS_REF="SIMPLE_LABEL" VISIBLE="false"/>
				<COMPONENT FIELDNAME="DETAILS" CLASS_REF="SIMPLE_LABEL" VISIBLE="false">
					<PROPERTIES>
						<PROPERTY NAME="STYLE" VALUE="white-space: normal;"/>
					</PROPERTIES>
				</COMPONENT>
				<COMPONENT FIELDNAME="TRANSACTION" VISIBLE="false">
					<PROPERTIES>
						<PROPERTY NAME="LABEL" LVALUE="TRANSACTION"/>
						<PROPERTY NAME="COLSPAN" VALUE="2"/>
					</PROPERTIES>
				</COMPONENT>
				<COMPONENT FIELDNAME="HOLD" VISIBLE="false">
					<PROPERTIES>
						<PROPERTY NAME="LABEL" LVALUE="HOLD"/>
						<PROPERTY NAME="COLSPAN" VALUE="2"/>
					</PROPERTIES>
				</COMPONENT>
			</COMPONENTS>
		</WIDGET>

	</WIDGETS>
</FORM>